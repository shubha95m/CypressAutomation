{
  "name": "cypress_automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "node_modules/.bin/cypress",
    "test": "node_modules\\.bin\\cypress run",
    "greenkartHeaded": "npm run test -- --spec cypress/integration/Practice/Greenkart.js --headed",
